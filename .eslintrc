{
	"env": {
		"browser": true,
		"es6": true,
		"jest": true
	},
	"extends": "liferay",
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2017,
		"sourceType": "module"
	},
	"plugins": [
		"react"
	],
	"rules": {
		"require-jsdoc": "off",
		"no-unused-vars": "off"
	}
}